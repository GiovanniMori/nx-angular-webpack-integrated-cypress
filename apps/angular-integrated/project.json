{
  "name": "angular-integrated",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "nx",
  "sourceRoot": "apps/angular-integrated/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "ngx-build-plus:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/angular-integrated",
        "index": "apps/angular-integrated/src/index.html",
        "main": "apps/angular-integrated/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/angular-integrated/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/angular-integrated/src/favicon.ico",
          "apps/angular-integrated/src/assets"
        ],
        "styles": [
          "apps/angular-integrated/src/styles.scss"
        ],
        "scripts": [],
        "extraWebpackConfig": "apps/angular-integrated/webpack.config.js",
        "commonChunk": false
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "extraWebpackConfig": "apps/angular-integrated/webpack.prod.config.js"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "ngx-build-plus:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "angular-integrated:build:production",
          "extraWebpackConfig": "apps/angular-integrated/webpack.prod.config.js"
        },
        "development": {
          "buildTarget": "angular-integrated:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4200,
        "publicHost": "http://localhost:4200",
        "extraWebpackConfig": "apps/angular-integrated/webpack.config.js"
      }
    },
    "extract-i18n": {
      "executor": "ngx-build-plus:extract-i18n",
      "options": {
        "buildTarget": "angular-integrated:build",
        "extraWebpackConfig": "apps/angular-integrated/webpack.config.js"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/angular-integrated/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "angular-integrated:build"
      }
    }
  }
}